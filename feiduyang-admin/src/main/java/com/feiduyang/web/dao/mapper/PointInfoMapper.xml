<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feiduyang.web.dao.management.PointInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.feiduyang.common.entity.management.PointInfo">
        <result column="id" property="id"/> <!--  -->
        <result column="creator_id" property="creatorId"/> <!--  -->
        <result column="last_modifier_id" property="lastModifierId"/> <!--  -->
        <result column="create_time" property="createTime"/> <!--  -->
        <result column="update_time" property="updateTime"/> <!--  -->
        <result column="t_point_infocol1" property="tPointInfocol1"/> <!--  -->
        <result column="point_name" property="pointName"/> <!-- 点位名称 -->
        <result column="point_no" property="pointNo"/> <!-- 点位编号 -->
        <result column="point_address" property="pointAddress"/> <!-- 点位地址 -->
        <result column="point_address_code" property="pointAddressCode"/> <!-- 点位地址编码 -->
        <result column="point_device_no" property="pointDeviceNo"/> <!-- 点位设备编号 -->
        <result column="point_business_id" property="pointBusinessId"/> <!-- 点位所属商户 -->
        <result column="point_maintenance_person" property="pointMaintenancePerson"/> <!-- 点位维护人员 -->
        <result column="point_maintenance_person_phone" property="pointMaintenancePersonPhone"/> <!-- 点位维护人员电话 -->
        <result column="point_charg_company" property="pointChargCompany"/> <!-- 点位所属单位 -->
        <result column="point_capacity" property="pointCapacity"/> <!-- 点位规模（有多少辆车） -->
        <result column="point_status" property="pointStatus"/> <!-- 点位状态 -->
        <result column="point_temporary_fee" property="pointTemporaryFee"/> <!-- 点位临时停车费用 -->
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        t_point_infocol1, point_name, point_no, point_address, point_address_code, point_device_no, point_business_id, point_maintenance_person, point_maintenance_person_phone, point_charg_company, point_capacity, point_status, point_temporary_fee
    </sql>
    <!-- 通用查询条件 -->
    <sql id="Find_List_Where">
        <where>
            <if test="m.id != null">
                AND id = #{m.id}  <!--  -->
            </if>
            <if test="m.creatorId != null">
                AND creator_id = #{m.creatorId}  <!--  -->
            </if>
            <if test="m.lastModifierId != null">
                AND last_modifier_id = #{m.lastModifierId}  <!--  -->
            </if>
            <if test="m.createTime != null">
                AND create_time = #{m.createTime}  <!--  -->
            </if>
            <if test="m.updateTime != null">
                AND update_time = #{m.updateTime}  <!--  -->
            </if>
            <if test="m.tPointInfocol1 != null and m.tPointInfocol1 != ''">
                AND t_point_infocol1 = #{m.tPointInfocol1}  <!--  -->
            </if>
            <if test="m.pointName != null and m.pointName != ''">
                AND point_name = #{m.pointName}  <!-- 点位名称 -->
            </if>
            <if test="m.pointNo != null and m.pointNo != ''">
                AND point_no = #{m.pointNo}  <!-- 点位编号 -->
            </if>
            <if test="m.pointAddress != null and m.pointAddress != ''">
                AND point_address = #{m.pointAddress}  <!-- 点位地址 -->
            </if>
            <if test="m.pointAddressCode != null and m.pointAddressCode != ''">
                AND point_address_code = #{m.pointAddressCode}  <!-- 点位地址编码 -->
            </if>
            <if test="m.pointDeviceNo != null and m.pointDeviceNo != ''">
                AND point_device_no = #{m.pointDeviceNo}  <!-- 点位设备编号 -->
            </if>
            <if test="m.pointBusinessId != null">
                AND point_business_id = #{m.pointBusinessId}  <!-- 点位所属商户 -->
            </if>
            <if test="m.pointMaintenancePerson != null and m.pointMaintenancePerson != ''">
                AND point_maintenance_person = #{m.pointMaintenancePerson}  <!-- 点位维护人员 -->
            </if>
            <if test="m.pointMaintenancePersonPhone != null and m.pointMaintenancePersonPhone != ''">
                AND point_maintenance_person_phone = #{m.pointMaintenancePersonPhone}  <!-- 点位维护人员电话 -->
            </if>
            <if test="m.pointChargCompany != null and m.pointChargCompany != ''">
                AND point_charg_company = #{m.pointChargCompany}  <!-- 点位所属单位 -->
            </if>
            <if test="m.pointCapacity != null">
                AND point_capacity = #{m.pointCapacity}  <!-- 点位规模（有多少辆车） -->
            </if>
            <if test="m.pointStatus != null">
                AND point_status = #{m.pointStatus}  <!-- 点位状态 -->
            </if>
            <if test="m.pointTemporaryFee != null">
                AND point_temporary_fee = #{m.pointTemporaryFee}  <!-- 点位临时停车费用 -->
            </if>
        </where>
        ORDER BY id ASC
    </sql>

